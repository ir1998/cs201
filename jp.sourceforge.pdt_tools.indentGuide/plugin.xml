<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.ui.preferencePages.GeneralTextEditor"
            class="jp.sourceforge.pdt_tools.indentguide.preferences.IndentGuidePreferencePage"
            id="jp.sourceforge.pdt_tools.indentguide.preferences.PreferencePage"
            name="%page.name">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="jp.sourceforge.pdt_tools.indentguide.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="jp.sourceforge.pdt_tools.indentguide.Starter">
      </startup>
   </extension>

</plugin>
