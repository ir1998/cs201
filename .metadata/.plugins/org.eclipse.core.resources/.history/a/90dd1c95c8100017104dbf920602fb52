package messages;

public class NewGameRequest implements Serializable{
	public String name;
	public int players;
	public String hostName; //host username
	public String phrase;
	
	public NewGameRequest(name, players, hostname, phrase){
		this.name = name;
		this.players = players;
		this.hostName = hostname;
		this.phrase = phrase;
	}
	
	
}
