package messages;

import java.io.Serializable;

public class NewGameRequest implements Serializable{
	public String name;
	public int players;
	public String hostName; //host username
	public String phrase;
	
	public NewGameRequest(String name, int players, String hostname, String phrase){
		this.name = name;
		this.players = players;
		this.hostName = hostname;
		this.phrase = phrase;
	}
	
	
}
